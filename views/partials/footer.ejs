    <script>
        document.addEventListener("DOMContentLoaded", function() {
            const schoolNameElement = document.querySelector('.navbar-brand h1');
            const originalSchoolName = schoolNameElement.textContent;
            const smallScreenSchoolName = "JAIN SAMNOPASAK SCHOOL";
            const navbarToggler = document.querySelector('.navbar-toggler');
        
            function updateSchoolNameBasedOnNavbarVisibility() {
                // Check if the navbar-toggler is visible
                const isTogglerVisible = navbarToggler.offsetParent !== null;
                
                // Update the school name based on the visibility of the navbar-toggler
                schoolNameElement.textContent = isTogglerVisible ? smallScreenSchoolName : originalSchoolName;
            }
        
            // Listen for window resize events
            window.addEventListener('resize', updateSchoolNameBasedOnNavbarVisibility);
        
            // Initial check in case the page loads in a small viewport
            updateSchoolNameBasedOnNavbarVisibility();
        });
        </script>
